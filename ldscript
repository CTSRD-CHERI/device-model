TARGET(elf64-tradbigmips)
ENTRY(_start)

SECTIONS
{
	. = 0xffffffffb0000000;
	.start . : {
		*start.o(.text)
	}

	. = 0xffffffffb0001000;
	.exception . : {
		*exception.o(.text)
	}

	.text : {
		*(.exception)
		*(.text)
	}

	.rodata : {
		*(.rodata)
	}

	.data : {
		*(.data)
	}

	.sdata : {
		_gp = .;
		*(.sdata)
	}

	.bss : {
		_sbss = ABSOLUTE(.);
		*(.bss COMMON)
		_ebss = ABSOLUTE(.);
	}
}
